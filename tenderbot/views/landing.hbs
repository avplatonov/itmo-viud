<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Tendermap</title>
    <style>
        body {
            margin: 0;
        }
        div.interactive-map {
            display: flex;
            justify-content: center;
        }
        button.region-chooser {
            position: absolute;
            width: 8px;
            height: 10px;
            border-radius: 50%;
            border: none;
            background-color: magenta;
            padding: 10px;
        }
    </style>
    <script>
        window.onload = function() {
            document.getElementById('region_button_wk').addEventListener('click', pressRegion('wk'));
        }
        function pressRegion(region) {
            switch (region) {
                case 'wk':
                alert('12')
                    console.log('wk');
                    break;
                case 'a':
                    console.log('a');
                    break;
            }
        }
    </script>
</head>
<body>
    <div class="interactive-map">
        <img class="map-image" src="https://simplemaps.com/static/svg/kz/kz.svg">
        <button class="region-chooser" id="region_button_wk"></button>
    </div>
</body>
</html>